# munin commands completion
#
_munin_list_plugins() {
	COMPREPLY=()
    cur=${COMP_WORDS[COMP_CWORD]}

	local PLUGIN_LIST
	PLUGIN_LIST=$(ls @@CONFDIR@@/plugins 2>&1)
	COMPREPLY=( $( compgen -W "$PLUGIN_LIST" -- "$cur" ) )
}

complete -F _munin_list_plugins munindoc
complete -F _munin_list_plugins munin-run

